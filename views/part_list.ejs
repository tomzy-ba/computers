<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/style.css" />
    </head>
    <body>
        <h1><%= title %></h1>
        <div>
            <% partProps.forEach(prop => { %>
                <div>
                    <p><%= prop.name %></p>
                    <p><%= prop.type %></p>
                    <p><%= prop.brand.name %></p>
                </div>
            <% }) %>
        </div>
    </body>
</html>

<style>
    div, p{
        border: 1px solid black;
        padding: 10px;
    }
</style>